{
	"datacenter": "east-aws",
	"data_dir": "/opt/consul",
	"log_level": "INFO",
	"node_name": "consul",
	"server": true,
	"addresses": {
	  "https": "0.0.0.0"
	},
	"ports": {
	  "https": 8080
	},
	"key_file": "/etc/pki/tls/private/consul.key.pem",
	"cert_file": "/etc/pki/tls/certs/consul.cert.pem",
	"ca_file": "/etc/pki/tls/certs/ca-chain.cert.pem"
  }